{
  "name": "microhabit",
  "version": "1.0.0",
  "description": "The 2-Minute Life Compound Engine with AI",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node --watch server/index.js",
    "dev:client": "cd client && node ../node_modules/vite/bin/vite.js --port 3003",
    "start": "node server/index.js",
    "build": "cd client && node ../node_modules/vite/bin/vite.js build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.28.0",
    "dotenv": "^16.4.1",
    "concurrently": "^8.2.2",
    "vite": "^5.1.0"
  }
}
